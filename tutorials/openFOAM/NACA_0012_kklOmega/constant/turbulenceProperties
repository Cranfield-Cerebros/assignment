    decayControl    $decayControl;
    kInf            $kInf;
    omegaInf        $omegaInf;
    ca1             $ca1;
    ca2             $ca2;
    ce1             $ce1;
    ce2             $ce2;
    cThetat         $cThetat;
    sigmaThetat     $sigmaThetat;
    A0              $A0;
    As              $As;
    Av              $Av;
    Abp             $Abp;
    Anat            $Anat;
    Ats             $Ats;
    CbpCrit         $CbpCrit;
